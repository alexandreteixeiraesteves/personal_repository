(*--------------------------------------------------*)
(* Some int-specific functions                      *)
(*--------------------------------------------------*)
fun int_is_odd (i:int) returns (o:bool)

(*--------------------------------------------------*)
(* Convert int to float (needed when reading data). *)
(*--------------------------------------------------*)
fun int2float(i:int) returns (o:float)
fun float2int(i:float) returns (o:int)

(*--------------------------------------------------*)
(* External functions on float arguments.           *)
(*--------------------------------------------------*)
fun float_round(i:float) returns (o:float)
fun float_asin(i:float) returns (o:float)
fun float_acos(i:float) returns (o:float)
fun float_atan2(im:float;re:float) returns (ang:float)
fun float_sqrt(i:float) returns (o:float)
fun float_sin(i:float) returns (o:float)
fun float_cos(i:float) returns (o:float)
fun float_pow(i:float;j:float) returns (o:float)
